    "@prisma/client": "^5.11.0",
    "dotenv": "^16.4.5",
    "hono": "^4.1.4",
    "inngest": "^3.16.1",
    "kysely": "^0.27.3",
    "nanoid": "^3.3.7",
    "pg": "^8.11.3",
    "zod": "^3.22.4",
    "gummy": "^0.0.28"
  },
  "devDependencies": {
    "@lr/eslint-config": "workspace:*",
    "@lr/typescript-config": "workspace:*",
    "@quramy/prisma-fabbrica": "^2.1.1",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.4",
    "eslint": "^8.57.0",
    "prisma": "5.12.1",
    "prisma-kysely": "^1.8.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.4.0"
  }
}
