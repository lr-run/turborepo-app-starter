{
  "name": "@lr/server",
  "version": "0.1.0",
  "private": true,
  "exports": {
    "./rpc": "./src/rpc.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc --build",
    "start": "node dist/index.js",
    "lint": "eslint --ext js,ts,jsx,tsx --max-warnings=0 .",
    "lint:fix": "eslint --ext js,ts,jsx,tsx . --fix",
    "db:migrate": "prisma migrate dev",
